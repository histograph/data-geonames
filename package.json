{
  "name": "histograph-data-geonames",
  "version": "0.6.0",
  "description": "Download and convert GeoNames data to Histograph NDJSON",
  "main": "geonames.js",
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "tape": "^3.4.0"
  },
  "scripts": {
    "test": "jshint *.js && jscs *.js"
  },
  "author": "Bert Spaan",
  "license": "MIT",
  "dependencies": {
    "highland": "^2.5.1",
    "ramda": "^0.18.0",
    "request": "^2.67.0",
    "yauzl": "^2.4.1"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "disallowMultipleVarDecl": true,
    "requireMultipleVarDecl": null
  }
}
